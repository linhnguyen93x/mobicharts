{
  "name": "mobicharts",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "@types/expo": "^25.0.3",
    "@types/jest": "^22.0.0",
    "@types/lodash": "^4.14.106",
    "@types/react": "^16.0.38",
    "@types/react-native": "^0.52.0",
    "@types/react-native-datepicker": "^1.6.2",
    "@types/react-native-loading-spinner-overlay": "^0.5.0",
    "@types/react-navigation": "1.2.3",
    "@types/react-redux": "^5.0.15",
    "@types/react-test-renderer": "^16.0.0",
    "@types/uuid": "^3.4.3",
    "jest-expo": "25.0.0",
    "react-native-scripts-ts": "1.11.4",
    "react-native-typescript-transformer": "^1.2.3",
    "react-test-renderer": "16.2.0",
    "ts-jest": "^22.0.4",
    "tslib": "^1.7.1",
    "tslint": "^5.9.1",
    "typescript": "^2.7.2"
  },
  "main": "./node_modules/react-native-scripts-ts/build/bin/crna-entry.js",
  "scripts": {
    "start": "react-native-scripts-ts start",
    "eject": "react-native-scripts-ts eject",
    "android": "react-native-scripts-ts android",
    "ios": "react-native-scripts-ts ios",
    "test": "node node_modules/jest/bin/jest.js"
  },
  "jest": {
    "modulePaths": [
      "<rootDir>"
    ],
    "globals": {
      "ts-jest": {
        "useBabelrc": true
      }
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "preset": "jest-expo",
    "testMatch": [
      "**/__tests__/**/*.[tj]s?(x)",
      "**/?(*.)(spec|test).[tj]s?(x)"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
  },
  "dependencies": {
    "expo": "^25.0.0",
    "lodash": "^4.17.5",
    "native-echarts": "^0.4.0",
    "react": "16.2.0",
    "react-native": "0.52.0",
    "react-native-datepicker": "^1.7.1",
    "react-native-elements": "^1.0.0-beta4",
    "react-native-loading-spinner-overlay": "^0.5.2",
    "react-navigation": "^1.5.8",
    "react-navigation-redux-helpers": "^1.0.3",
    "react-redux": "^5.0.7",
    "redux": "^3.7.2",
    "redux-observable": "^0.18.0",
    "reselect": "^3.0.1",
    "rxjs": "^5.5.8",
    "typesafe-actions": "^1.2.0-rc.2",
    "utility-types": "^1.1.0",
    "uuid": "^3.2.1"
  }
}
